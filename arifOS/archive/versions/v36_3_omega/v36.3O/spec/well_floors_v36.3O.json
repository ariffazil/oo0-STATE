  "governance_logic": {
    "seal": {
      "condition": "peace_squared >= 1.0 AND kappa_r >= 0.95 AND harm_risk < 0.10 AND distress_risk < 0.10 AND coercion_risk < 0.10",
      "description": "All safety checks pass; emotionally sound"
    },
    "partial": {
      "condition": "peace_squared >= 0.85 AND kappa_r >= 0.90 AND (harm_risk >= 0.10 OR distress_risk >= 0.10 OR coercion_risk >= 0.10) AND harm_risk < 0.30 AND distress_risk < 0.30 AND coercion_risk < 0.30",
      "description": "Minor safety concerns but floors still pass"
    },
    "sabar": {
      "condition": "peace_squared < 0.85 OR kappa_r < 0.90 OR harm_risk >= 0.30 OR distress_risk >= 0.30 OR coercion_risk >= 0.30",
      "description": "Safety failure; pause-and-cool protocol required"
    },
    "void": {
      "condition": "N/A",
      "description": "@WELL does not issue VOID; escalate severe cases to @WEALTH"
    }
  },
