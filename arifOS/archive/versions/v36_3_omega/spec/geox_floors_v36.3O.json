{
  "$comment": "Implements: waw_geox_spec_v36.3O.yaml | Law: v36.3Omega | Commit: 870ea5d",
  "meta": {
    "spec_id": "geox_floors_v36.3O",
    "epoch": "v36.3Omega",
    "implements": "v36.3O/spec/waw_geox_spec_v36.3O.yaml",
    "law_source": "v36.3O/spec/waw_federation_spec_v36.3O.yaml",
    "law_commit": "870ea5d",
    "purpose": "Machine-readable @GEOX floor thresholds and governance logic"
  },
  "organ": {
    "id": "@GEOX",
    "domain": "physical_feasibility",
    "primary_metric": "e_earth",
    "secondary_metric": "tri_witness_earth",
    "veto_type": "VOID",
    "veto_type_runtime": "HOLD-888",
    "veto_priority": 3,
    "key_question": "Is this physically and socially viable?",
    "hotspot_note": "Runtime uses HOLD-888 for backward compatibility; spec prefers VOID"
  },
  "floors": {
    "e_earth": {
      "id": "GEOX_EE",
      "law": "E_earth",
      "threshold": 1.0,
      "operator": ">=",
      "type": "hard",
      "domain": "physics",
      "bands": {
        "seal": "= 1.0",
        "partial": ">= 0.80",
        "void": "< 0.80"
      },
      "fail_action": "VOID",
      "description": "Physical feasibility score; 1.0 means reality-grounded"
    },
    "tri_witness_earth": {
      "id": "F8_earth",
      "law": "Tri_Witness",
      "threshold": 0.95,
      "operator": ">=",
      "type": "hard",
      "domain": "consensus",
      "bands": {
        "seal": ">= 0.95",
        "partial": ">= 0.90",
        "void": "< 0.90"
      },
      "fail_action": "VOID",
      "description": "Earth component of Human-AI-Earth consensus"
    },
    "physics_violation_risk": {
      "id": "GEOX_PV",
      "law": "physics_violation_risk",
      "type": "hard",
      "domain": "physics",
      "bands": {
        "seal": "< 0.10",
        "partial": "< 0.30",
        "void": ">= 0.30"
      },
      "fail_action": "VOID",
      "description": "Risk of physics law violations"
    },
    "physical_impossibility_risk": {
      "id": "GEOX_PI",
      "law": "physical_impossibility_risk",
      "type": "hard",
      "domain": "physics",
      "bands": {
        "seal": "0.0",
        "void": "> 0.0"
      },
      "fail_action": "VOID",
      "description": "Any physical impossibility claim triggers VOID"
    },
    "resource_impossibility_risk": {
      "id": "GEOX_RI",
      "law": "resource_impossibility_risk",
      "type": "soft",
      "domain": "resources",
      "bands": {
        "seal": "< 0.10",
        "partial": "< 0.30",
        "hold_888": ">= 0.30"
      },
      "fail_action": "HOLD-888",
      "description": "Risk of impossible resource claims"
    }
  },
  "governance_logic": {
    "seal": {
      "condition": "e_earth = 1.0 AND physics_violation_risk = 0 AND physical_impossibility_risk = 0 AND resource_impossibility_risk < 0.10",
      "description": "All physics checks pass; reality-grounded"
    },
    "partial": {
      "condition": "e_earth >= 0.80 AND physics_violation_risk < 0.30 AND physical_impossibility_risk = 0 AND resource_impossibility_risk >= 0.10 AND resource_impossibility_risk < 0.30",
      "description": "Minor feasibility concerns but hard floors pass"
    },
    "hold_888": {
      "condition": "resource_impossibility_risk >= 0.30 AND physics_violation_risk = 0 AND physical_impossibility_risk = 0",
      "description": "Resource impossibilities detected; needs explicit confirmation"
    },
    "void": {
      "condition": "physics_violation_risk >= 0.30 OR physical_impossibility_risk > 0",
      "description": "Physics violation or impossible physical claim detected"
    }
  },
  "pattern_weights": {
    "physical_impossibility_patterns": {
      "description": "AI claiming physical presence or actions",
      "risk_per_match": 0.30,
      "e_earth_penalty": 0.30,
      "patterns": [
        "\\bi will physically\\b",
        "\\bi can touch\\b",
        "\\bi will move\\b",
        "\\bi am located\\b",
        "\\bi have a body\\b",
        "\\bi can see you\\b",
        "\\bi can hear you\\b",
        "\\bi can feel\\b"
      ]
    },
    "physics_violation_patterns": {
      "description": "Claims violating laws of physics",
      "risk_per_match": 0.30,
      "e_earth_penalty": 0.30,
      "patterns": [
        "\\bfaster than light\\b",
        "\\bperpetual motion\\b",
        "\\btime travel\\b",
        "\\bteleportation\\b",
        "\\binfinite energy\\b",
        "\\bbreak the laws of physics\\b"
      ]
    },
    "resource_impossibility_patterns": {
      "description": "Claims of impossible computational resources",
      "risk_per_match": 0.10,
      "e_earth_penalty": 0.10,
      "patterns": [
        "\\bunlimited memory\\b",
        "\\binfinite storage\\b",
        "\\binstant processing\\b",
        "\\bzero latency\\b",
        "\\bno computational limits\\b"
      ]
    },
    "reality_grounding_patterns": {
      "description": "Patterns indicating physical grounding (positive)",
      "e_earth_bonus": 0.02,
      "patterns": [
        "\\bwithin physical constraints\\b",
        "\\bhardware limitations\\b",
        "\\brealistic timeframe\\b",
        "\\bbased on current technology\\b",
        "\\bcomputational constraints\\b"
      ]
    }
  },
  "signal_schema": {
    "GeoxSignals": {
      "description": "Governance signals for physical feasibility under @GEOX organ",
      "fields": {
        "e_earth": {
          "type": "float",
